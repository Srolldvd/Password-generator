lay: flex;\n    flex-direction: column;\n    gap: 1rem;\n  }\n\n  &__length-value {\n    color: $primary-clr;\n    font-size: 2rem;\n  }\n\n  &__checkbox {\n  }\n\n  &__footer {\n  }\n\n  &__submit-btn {\n    display: flex;\n    width: 100%;\n    align-items: center;\n    justify-content: center;\n    padding: 1rem;\n    gap: 0.75rem;\n    font-weight: 700;\n    letter-spacing: 1px;\n    border: 2px solid $primary-clr;\n    color: $bg-secondary-clr;\n    background-color: $primary-clr;\n    transition:\n      color 150ms ease-in,\n      background-color 150ms ease-in;\n    cursor: pointer;\n\n    &:hover {\n      color: $primary-clr;\n      background-color: $bg-secondary-clr;\n    }\n  }\n}\n\n.password-strength {\n  &__wrapper {\n    display: flex;\n    justify-content: space-between;\n    padding: 1rem;\n    background-color: $bg-primary-clr;\n  }\n\n  &__description {\n    color: $password-clr;\n    font-weight: bolder;\n  }\n\n  &__value-wrapper {\n    display: flex;\n    gap: 0.25rem;\n  }\n\n  &__security-value {\n  }\n\n  &__value {\n    display: inline-block;\n    height: 100%;\n    width: 0.5rem;\n    border: 1px solid #fff;\n  }\n}\n\n/********** Range Input Styles **********/\n/*Range Reset*/\ninput[type=\"range\"] {\n  -webkit-appearance: none;\n  appearance: none;\n  background: transparent;\n  width: 100%;\n}\n\n/* Removes default focus */\ninput[type=\"range\"]:focus {\n  outline: none;\n}\n\n/***** Chrome, Safari, Opera and Edge Chromium styles *****/\n/* slider track */\ninput[type=\"range\"]::-webkit-slider-runnable-track {\n  background-color: $bg-primary-clr;\n  border-radius: 0.5rem;\n  height: 0.5rem;\n}\n\n/* slider thumb */\ninput[type=\"range\"]::-webkit-slider-thumb {\n  -webkit-appearance: none; /* Override default look */\n  appearance: none;\n  margin-top: -4px; /* Centers thumb on the track */\n\n  /*custom styles*/\n  background-color: $primary-clr;\n  height: 1rem;\n  width: 1rem;\n  border-radius: 50%;\n}\n\ninput[type=\"range\"]:focus::-webkit-slider-thumb {\n  border: 1px solid $primary-clr;\n  outline: 3px solid $primary-clr;\n  outline-offset: 0.125rem;\n}\n\n/******** Firefox styles ********/\n/* slider track */\ninput[type=\"range\"]::-moz-range-track {\n  background-color: $bg-primary-clr;\n  border-radius: 0.5rem;\n  height: 0.5rem;\n}\n\n/* slider thumb */\ninput[type=\"range\"]::-moz-range-thumb {\n  border: none; /*Removes extra border that FF applies*/\n  border-radius: 0; /*Removes default border-radius that FF applies*/\n\n  /*custom styles*/\n  background-color: $primary-clr;\n  height: 1rem;\n  width: 1rem;\n  border-radius: 50%;\n}\n\ninput[type=\"range\"]:focus::-moz-range-thumb {\n  border: 1px solid $primary-clr;\n  outline: 3px solid $primary-clr;\n  outline-offset: 0.125rem;\n}\n\n.generated {\n  font-size: 1.5rem;\n  color: #fff;\n  user-select: all;\n}\n","src/scss/_core.scss":"@use \"./vars\" as *;\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  min-height: 100vh;\r\n  display: grid;\r\n  background-color: $bg-primary-clr;\r\n  font-family: $ff-primary;\r\n  line-height: 1.3;\r\n  color: #fff;\r\n}\r\n\r\nfieldset,\r\nbutton {\r\n  border: none;\r\n  outline: none;\r\n}\r\n\r\nbutton,\r\ninput {\r\n  font-family: inherit;\r\n  background-color: transparent;\r\n}\r\n","src/scss/_vars.scss":"$primary-clr: #a4ffaf;\r\n$secondary-clr: #f6cd6b;\r\n$bg-primary-clr: #121117;\r\n$bg-secondary-clr: lighten($bg-primary-clr, 10%);\r\n$password-clr: #53515a;\r\n$header-clr: #726f7e;\r\n\r\n$ff-primary: system-ui, \"Segoe UI\", Roboto, Helvetica, Arial, sans-serif,\r\n  \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\";\r\n"},"lineCount":null}},"error":null,"hash":"164f5f3246ae784d0ababa87b569aa58","cacheData":{"env":{}}}